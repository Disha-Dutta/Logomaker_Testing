orage.set(logoSelected);\n      Logo.instance = new Logo(storage.get(logoSelected));\n      this.$router.push({ path: \"/logo-maker\" });\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n@import \"./form.scss\";\n</style>\n"],"sourceRoot":"src/components/CreateLogo/Form"}]}